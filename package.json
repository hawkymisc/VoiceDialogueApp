{
  "name": "voice-dialogue-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "web": "webpack serve --config webpack.config.js",
    "build:web": "webpack --config webpack.config.js --mode production",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:all": "npm run test && npm run test:integration",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.45.0",
    "openai": "^5.10.1",
    "react": "18.2.0",
    "react-dom": "^18.3.1",
    "react-native": "0.72.15",
    "react-native-elements": "^3.4.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-sound": "^0.11.2",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.19.13",
    "react-redux": "^9.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.20.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^10.0.0",
    "core-js": "^3.44.0",
    "eslint": "^8.19.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^30.0.4",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons|react-native-elements|react-native-gesture-handler|react-native-reanimated|react-native-safe-area-context|react-native-screens|react-native-sound|react-native-fs|@react-navigation)/)"
    ],
    "testPathIgnorePatterns": [
      "/src/services/__tests__/live2dService.test.ts",
      "/src/components/__tests__/Live2DViewer.test.tsx",
      "/src/store/slices/__tests__/live2dSlice.test.ts",
      "/src/hooks/__tests__/useLive2D.test.ts",
      "/src/services/__tests__/contentFilterService.test.ts",
      "/src/services/__tests__/securityService.test.ts",
      "/src/__tests__/integration/progressionService.integration.test.ts",
      "/src/__tests__/integration/dailyContentService.integration.test.ts",
      "/src/components/__tests__/CharacterCustomization.test.tsx",
      "/src/components/__tests__/SettingsScreen.test.tsx",
      "/src/components/__tests__/DataManagementScreen.test.tsx",
      "/src/__tests__/integration/enhanced.integration.test.ts",
      "/src/__tests__/integration/security.integration.test.ts",
      "/src/__tests__/integration/contentFilter.integration.test.ts",
      "/src/__tests__/integration/services.integration.test.ts",
      "/src/__tests__/integration/fullSystem.integration.test.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom"
  }
}
